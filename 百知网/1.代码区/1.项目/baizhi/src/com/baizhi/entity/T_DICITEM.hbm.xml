<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class entity-name="T_DICITEM" table="T_DICITEM" lazy="false">
		<comment>字典列表清单</comment>
		<id name="DICITEM_ID" column="DICITEM_ID" type="int" >
			<generator class="identity" />
		</id>
		<property name="CODE" type="string">
			<column name="CODE" length="50" >
				<comment>列表字典代码</comment>
			</column>
		</property>
		<property name="DIC_CODE" type="string">
			<column name="DIC_CODE" length="30" >
				<comment>字典代码</comment>
			</column>
		</property>
		<property name="DIC_NAME" type="string">
			<column name="DIC_NAME" length="100" >
				<comment>字典名称</comment>
			</column>
		</property>
		<property name="PDIC_CODE" type="string">
			<column name="PDIC_CODE" length="30" >
				<comment>字典上级代码</comment>
			</column>
		</property>
		<property name="ALLPIN" type="string">
			<column name="ALLPIN" length="100" >
				<comment>字典全拼</comment>
			</column>
		</property>
		<property name="SIMPLEPIN" type="string">
			<column name="SIMPLEPIN" length="30" >
				<comment>字典简拼</comment>
			</column>
		</property>
		<property name="ORDER_BY" type="int">
			<column name="ORDER_BY"  >
				<comment>显示顺序</comment>
			</column>
		</property>
		<property name="REMARK" type="string">
			<column name="REMARK" length="1000" >
				<comment>备注</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>