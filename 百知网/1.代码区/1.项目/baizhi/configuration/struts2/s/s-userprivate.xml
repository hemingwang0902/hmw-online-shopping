<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="userprivate" extends="json-default" namespace="/userprivate">
		<action name="getUserPrivateList" class="com.baizhi.userprivate.action.GetUserPrivateList">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="getUserPrivateByList" class="com.baizhi.userprivate.action.GetUserPrivateByList">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="getUserPrivateCount" class="com.baizhi.userprivate.action.GetUserPrivateCount">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
