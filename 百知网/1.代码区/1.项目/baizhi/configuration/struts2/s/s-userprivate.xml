<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="userprivate" extends="json-default" namespace="/userprivate">
		<action name="getUserPrivateList" class="com.baizhi.userprivate.action.GetUserPrivateList">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="initUserPrivateForm" class="com.baizhi.userprivate.action.InitUserPrivateForm">
			<result name="success" type="dispatcher">userprivateform.jsp</result>
		</action>
		<action name="saveUserPrivate" class="com.baizhi.userprivate.action.SaveUserPrivate">
			<result name="error"  type="dispatcher" >userprivateform.jsp</result>
			<result name="success" type="redirect" >userprivatelist.jsp</result>
			<result name="update" type="dispatcher" >userprivateform.jsp</result>
			<result name="invalid.token" type="freemarker">userprivateform.jsp</result> 
			<interceptor-ref name="defaultStack"><param name="workflow.excludeMethods">default</param></interceptor-ref> 
			<interceptor-ref name="tokenSession"><param name="excludeMethods">default</param></interceptor-ref>
		</action>
		<action name="delUserPrivate" class="com.baizhi.userprivate.action.DelUserPrivate">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="getUserPrivateById" class="com.baizhi.userprivate.action.GetUserPrivateById">
			<result name="success" type="dispatcher" >userprivateform.jsp</result>
		</action>
	</package>
</struts>