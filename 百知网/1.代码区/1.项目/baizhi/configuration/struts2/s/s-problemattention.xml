<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="problemattention" extends="json-default" namespace="/problemattention">
		<action name="getProblemAttentionList" class="com.baizhi.problemattention.action.GetProblemAttentionList">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="initProblemAttentionForm" class="com.baizhi.problemattention.action.InitProblemAttentionForm">
			<result name="success" type="dispatcher">problemattentionform.jsp</result>
		</action>
		<action name="saveProblemAttention" class="com.baizhi.problemattention.action.SaveProblemAttention">
			<result name="error"  type="dispatcher" >problemattentionform.jsp</result>
			<result name="success" type="redirect" >problemattentionlist.jsp</result>
			<result name="update" type="dispatcher" >problemattentionform.jsp</result>
			<result name="invalid.token" type="freemarker">problemattentionform.jsp</result> 
			<interceptor-ref name="defaultStack"><param name="workflow.excludeMethods">default</param></interceptor-ref> 
			<interceptor-ref name="tokenSession"><param name="excludeMethods">default</param></interceptor-ref>
		</action>
		<action name="delProblemAttention" class="com.baizhi.problemattention.action.DelProblemAttention">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="getProblemAttentionById" class="com.baizhi.problemattention.action.GetProblemAttentionById">
			<result name="success" type="dispatcher" >problemattentionform.jsp</result>
		</action>
	</package>
</struts>