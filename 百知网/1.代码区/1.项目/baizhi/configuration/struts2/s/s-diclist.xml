<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="diclist" extends="json-default" namespace="/diclist">
		<action name="getDiclistList" class="com.baizhi.diclist.action.GetDiclistList">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="initDiclistForm" class="com.baizhi.diclist.action.InitDiclistForm">
			<result name="success" type="dispatcher">diclistform.jsp</result>
		</action>
		<action name="saveDiclist" class="com.baizhi.diclist.action.SaveDiclist">
			<result name="error"  type="dispatcher" >diclistform.jsp</result>
			<result name="success" type="dispatcher" >diclistform.jsp</result>
			<result name="update" type="dispatcher" >diclistform.jsp</result>
			<result name="invalid.token" type="freemarker">diclistform.jsp</result> 
			<interceptor-ref name="defaultStack"><param name="workflow.excludeMethods">default</param></interceptor-ref> 
			<interceptor-ref name="tokenSession"><param name="excludeMethods">default</param></interceptor-ref>
		</action>
		<action name="delDiclist" class="com.baizhi.diclist.action.DelDiclist">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="getDiclistById" class="com.baizhi.diclist.action.GetDiclistById">
			<result name="success" type="dispatcher" >diclistform.jsp</result>
		</action>
		<action name="checkName" class="com.baizhi.diclist.action.CheckName">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="checkCode" class="com.baizhi.diclist.action.CheckCode">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
	</package>
</struts>