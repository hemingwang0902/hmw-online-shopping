<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="userattention" extends="json-default" namespace="/userattention">
		<action name="saveUserAttention" class="com.baizhi.userattention.action.SaveUserAttention">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
		<action name="cancelUserAttention" class="com.baizhi.userattention.action.CancelUserAttention">
			<result name="jsonsuccess" type="json">
		      <param name="root">result</param>
		    </result>
		</action>
	</package>
</struts>