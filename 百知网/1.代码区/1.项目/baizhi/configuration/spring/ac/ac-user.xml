<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="userDao" class="com.baizhi.user.dao.UserDao" parent="daoSupport">
		<property name="userLogDao" ref="userLogDao" ></property>
	</bean>
	
	<bean id="userService" class="com.baizhi.user.service.UserService" parent="serviceSupport">
		<property name="userDao">
			<ref local="userDao" />
		</property>
	</bean>
</beans>