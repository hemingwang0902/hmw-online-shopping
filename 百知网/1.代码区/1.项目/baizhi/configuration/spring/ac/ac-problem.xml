<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="problemDao" class="com.baizhi.problem.dao.ProblemDao" parent="daoSupport">
		<property name="userNoticeDao" ref="userNoticeDao"/>
		<property name="userDynamicDao" ref="userDynamicDao"/>
		<property name="userScoreDao" ref="userScoreDao"/>
	</bean>
	
	<bean id="problemService" class="com.baizhi.problem.service.ProblemService" parent="serviceSupport">
		<property name="problemDao">
			<ref local="problemDao" />
		</property>
	</bean>
</beans>