<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dwr="http://www.directwebremoting.org/schema/spring-dwr" xsi:schemaLocation="http://www.springframework.org/schema/beans       
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd 
    http://www.directwebremoting.org/schema/spring-dwr 
    http://www.directwebremoting.org/schema/spring-dwr-2.0.xsd">

	<!--上传文件资源路径-->		
	<bean id="uploadFilePath" class="com.baizhi.commons.configure.UploadFilePath" init-method="init">
		<!--上传照片、Logo-->
		<property name="logo_path" value="/upload/logo" />
		<!--上传品牌Logo-->
		<property name="brand_path" value="/upload/brand" />
	</bean>
</beans>